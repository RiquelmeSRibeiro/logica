Algoritmo "valordevendas"

Var

       VV:inteiro
       V:inteiro
       

Inicio

       escreva("Digite o valor que deve ser atingido:")
       leia(V)
       
       escreva("Me informe o valor que você conseguiu atingir:")
       leia(VV)
       
       se(V<=VV) entao
       escreva("Você ganhou!")
       senao
       escreva("Você perdeu...")
       fimse
       
       

Fimalgoritmo